<!--
	HIGHLIGHT OF THIS TEST
	======================
	- reference to undeclared complex type
-->
<schema xmlns="http://www.w3.org/2001/XMLSchema"
		xmlns:foo="foo"
		targetNamespace="foo"
		elementFormDefault="qualified">
	
	<complexType name="abc">
		<sequence>
			<any />
		</sequence>
	</complexType>
	
	<element name="root">
		<complexType>
			<complexContent>
				<extension base="abc">
					<sequence>
						<any />
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
</schema>
